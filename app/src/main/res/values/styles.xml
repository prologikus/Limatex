<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/ColorWhiteB</item>
        <item name="colorPrimaryDark">@color/ColorWhiteB</item>
        <item name="colorAccent">@color/colorAccent</item>

        <!-- Navigation bar color -->
        <item name="android:navigationBarColor">@color/colorBrown</item>
        <!-- Status bar color -->
        <item name="android:statusBarColor">@color/colorBrown</item>
        <!-- No action bar -->
        <item name="windowActionBar">false</item>
        <!-- No title on action bar -->
        <item name="windowNoTitle">true</item>

        <item name="android:textColorPrimary">@color/colorBrown</item>
        <item name="android:colorBackground">@color/ColorWhiteB</item>



    </style>

    <style name="CustomActionBarTheme"
        parent="Base.ThemeOverlay.AppCompat.Dark">
    </style>

</resources>
